{
	"name": "frontendforum",
	"private": true,
	"scripts": {
		"dev": "slidev",
		"dev:onboarding": "slidev slides-frontend-onboarding.md",
		"dev:a11y": "slidev slides-a11y-forum.md",
		"build": "slidev build",
		"export": "slidev export --per-slide --format pdf",
		"export:onboarding": "slidev export slides-frontend-onboarding.md --output frontend-onboarding.pdf --per-slide --format pdf",
		"export:a11y": "slidev export slides-frontend-onboarding.md --output frontend-a11y-forum.pdf --per-slide --format pdf",
		"lint:check": "biome check ./* --apply"
	},
	"dependencies": {
		"@slidev/cli": "0.48.8",
		"@slidev/theme-default": "^0.25.0",
		"slidev-theme-dracula": "^0.2.0",
		"slidev-theme-penguin": "^2.2.0",
		"slidev-theme-the-unnamed": "^0.0.16"
	},
	"devDependencies": {
		"@biomejs/biome": "1.6.4",
		"@iconify-json/la": "^1.1.8",
		"@iconify-json/logos": "^1.1.42",
		"@iconify-json/twemoji": "^1.1.15",
		"playwright-chromium": "1.42.1"
	}
}
