{
  "name": "frontendforum",
  "private": true,
  "homepage": "https://bellfalasch.github.io/frontend-forum",
  "scripts": {
    "dev": "slidev",
    "dev:onboarding": "slidev slides-frontend-onboarding.md",
    "build": "slidev build",
    "export": "slidev export --per-slide --format pdf",
    "export:onboarding": "slidev export slides-frontend-onboarding.md --output frontend-onboarding.pdf --per-slide --format pdf",
    "lint:check": "biome check ./* --apply",
    "predeploy": "bun run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@gjensidige/builders-fonts": "^1.0.12",
    "@gjensidige/builders-icons": "^1.7.1",
    "@gjensidige/builders-tokens": "^1.0.16",
    "@slidev/cli": "^0.49.29",
    "@slidev/theme-default": "^0.25.0",
    "slidev-theme-dracula": "^0.2.0",
    "slidev-theme-penguin": "^2.2.3",
    "slidev-theme-the-unnamed": "^0.0.21"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@iconify-json/la": "^1.2.0",
    "@iconify-json/logos": "^1.2.2",
    "@iconify-json/twemoji": "^1.2.0",
    "gh-pages": "^6.2.0",
    "playwright-chromium": "^1.47.2"
  }
}
